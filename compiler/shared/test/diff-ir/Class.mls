module Fn extends Callable {
  fun apply1(x) = builtin("println", x)
}
class Fn2(a) extends Callable {
  fun apply1(x) =
    builtin("println", a)
    builtin("println", x)
}
class Demo(n) {
  fun x() = n
}
fun f(fn) = fn(1)
fun main() =
  let d1 = Demo(2)
  Demo.x(d1)
  let print = Fn()
  Fn.apply1(print, 3)
  f(print)
  let print2 = Fn2(4)
  Fn2.apply1(print2, 5)
  print2(6)
  f(print2)
main()
