class Nat
class S(x) extends Nat
class Z extends Nat
fun pred(n) =
  if n is
    S(p) then p
    Z then Z
fun plus(n1, n2) =
  if n1 is
    Z then n2
    S(p) then S(plus(p, n2))
fun fib(n) =
  if n is
    Z then S(Z)
    S(p) then
      if p is
        Z then S(Z)
        S(q) then plus(fib(p), fib(q))
fun to_int(n) =
  if n is
    Z then 0
    S(p) then 1 + to_int(p)
fun to_nat(n) =
  if n == 0 then Z
  else S(to_nat(n - 1))
fun main() =
