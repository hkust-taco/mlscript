// fun toList: (Vec['l, 'a]) -> (['a, 'xs] as 'xs)
fun toList: (((Vec['l, 'a]) -> 'xs) where ['a, 'xs] | [] : 'xs)
fun toList(xs) = if xs is
    Nil then []
    Cons(h, t) then [h, toList(t)]
//│ fun toList: forall 'T 'xs. (Cons[?, out 'T] | Nil[?]) -> 'xs
//│ fun toList: forall 'l 'a 'xs0. Vec['l, 'a] -> 'xs0
//│ where
//│   'xs0 :> Array['a | 'xs0]
//│   'xs :> Array[??T & 'T | 'xs]

