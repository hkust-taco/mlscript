fun vecEq[L](xs: Vec[L, Int], ys: Vec[L, Int]): Bool =
    if xs is Nil and ys is Nil then true
    else if xs is Cons(x, tx) and ys is Cons(y, ty) then (x == y) && vecEq(tx, ty)
    else false
