:NewDefs
:NoJS


class Foo(x: Int) {
  fun bar = x
  fun baz: 1 | 2 = 1
}
//│ class Foo(x: Int) {
//│   fun bar: Int
//│   fun baz: 1 | 2
//│ }

let foo: Foo & { x: 0 | 1, bar: 0 | 1, baz: 0 | 1, y: Bool }
//│ let foo: Foo & {y: Bool, bar: 0 | 1, baz: 0 | 1, x: 0 | 1}

foo.x
//│ 0 | 1

foo.bar
//│ 0 | 1

foo.baz
//│ 1

foo.y
//│ Bool

