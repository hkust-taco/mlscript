:NewDefs

class Foo() {
  virtual fun f(x: Int) = x + 1
  val g: Int = 0
}
//│ class Foo() {
//│   fun f: (x: Int) -> Int
//│   let g: Int
//│ }

class Bar() extends Foo {
  fun f(x: Int) = x + 2
}
//│ class Bar() extends Foo {
//│   fun f: (x: Int) -> Int
//│   let g: Int
//│ }

Bar().f(40)
//│ Int
//│ res
//│     = 42

:e
class Baz() extends Foo {
  val g = 1
}
//│ ╔══[ERROR] Value member `g` is not virtual and cannot be overridden
//│ ║  l.27: 	  val g = 1
//│ ║        	      ^^^^^
//│ ╟── Declared here:
//│ ║  l.5: 	  val g: Int = 0
