//│ }

mixin X {
  fun f(x) = x + 42
}
mixin XX {
  fun f(x) = super.f(x) + 42
}
class T() extends X, XX
//│ mixin X() {
//│   fun f: Int -> Int
//│ }
//│ mixin XX() {
//│   super: {f: 'a -> Int}
//│   fun f: 'a -> Int
//│ }
//│ class T() {
//│   fun f: Int -> Int
//│ }

T().f(0 - 42)
//│ Int
//│ res
//│     = 42

class M1() {
  val foo: Str
}
class M2(s: Str) extends M1 {
  val foo = s
}
M2("foo").foo
//│ class M1() {
//│   let foo: Str
//│ }
//│ class M2(s: Str) extends M1 {
//│   let foo: Str
//│ }
//│ Str
//│ res
//│     = 'foo'

class U() {
  fun f: Int -> Int
  fun f(x) = x + 1
}
//│ class U() {
//│   fun f: Int -> Int
//│ }

:e
class V1() {
  virtual val x = 42
  fun foo = x // unqualified access!
}
//│ ╔══[ERROR] Unqualified access to virtual member x
//│ ║  l.91: 	  fun foo = x // unqualified access!
//│ ║        	      ^^^^^^^
//│ ╟── Declared here:
//│ ║  l.90: 	  virtual val x = 42
//│ ╙──      	              ^^^^^^
//│ class V1() {
//│   fun foo: 42
//│   let x: 42
//│ }

class V2() {
  val x: Int
  virtual val x = 42
  fun foo: Int = this.x
}
//│ class V2() {
//│   fun foo: Int
//│   let x: Int
//│ }


