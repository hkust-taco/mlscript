:NewDefs
:NoJS

trait Into[T] {
    fun into: T
}
//│ trait Into[T]() {
//│   fun into: T
//│ }

trait Nat extends Into[int]
//│ trait Nat() {
//│   fun into: 'T
//│ }
//│ where
//│   'T := int

trait Product[A, B] extends Into[A] {
    let pair: (A, B)
}
//│ trait Product[A, B]() {
//│   fun into: 'T
//│   let pair: (A, B,)
//│ }
//│ where
//│   'T := A

class TwoInts(pair: (int, int)) extends Product[int, int] {
    fun into = pair._1 + pair._2
}
//│ class TwoInts(pair: (int, int,)) {
//│   fun into: int
//│ }

let i2 = TwoInts((1,2))
//│ let i2: TwoInts

i2: Product[int, int]
//│ Product[int, int]

i2: Into[int]
//│ Into[int]

i2.pair
//│ (int, int,)

i2.into
//│ int

