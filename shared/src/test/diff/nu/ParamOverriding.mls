:NewDefs


mixin Over {
  fun p = "hi"
}
//│ mixin Over() {
//│   fun p: "hi"
//│ }


class Base1(p: Int) extends Over {
  fun test = [p, this.p]
  fun test2 = super.p
}
//│ class Base1(p: Int) {
//│   fun p: "hi"
//│   fun test: (Int, Int,)
//│   fun test2: Int
//│ }


Base1(123).test
//│ (Int, Int,)
//│ res
//│     = [ 123, 123 ]

Base1(123).test2
//│ Int
//│ res
//│     = 'hi'


