class A() {
  fun ma(x: Int, y: Int) = x - y
  fun mma(x: Int, y: Int) = y - x
} 
//│ class A() {
//│   fun ma: (x: Int, y: Int) -> Int
//│   fun mma: (x: Int, y: Int) -> Int
//│ }

let x = A()
x.ma(y: 2, x: 1)
//│ let x: A
//│ Int
//│ x
//│   = A {}
//│ res
//│     = -1

A().ma(x: 1, y: 2)
//│ Int
//│ res
//│     = -1

id(x).ma(y: 2, x: 1)
//│ Int
//│ res
//│     = -1


