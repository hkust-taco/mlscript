:NewDefs


x =>
y =>
x + y
//│ Int -> Int -> Int
//│ res
//│     = [Function: res]

id of x =>
y =>
x + y
//│ Int -> Int -> Int
//│ res
//│     = [Function (anonymous)]

let r = x =>
y =>
x + y
//│ let r: Int -> Int -> Int
//│ r
//│   = [Function: r]

r(1)(2)
//│ Int
//│ res
//│     = 3

(r of 1) of 2
//│ Int
//│ res
//│     = 3

