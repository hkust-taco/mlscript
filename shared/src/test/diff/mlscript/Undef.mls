
class Undef
//│ Defined class Undef

def undef = Undef{}
//│ undef: Undef
//│      = [Function: undef]


def example: int | Undef
//│ example: int | Undef
//│        = <missing implementation>

example = if true then undef else 42
//│ 42 | Undef
//│   <:  example:
//│ int | Undef
//│        = Undef {}

def qmrk_qmrk lhs rhs = case lhs of { Undef -> rhs | _ -> lhs }
//│ qmrk_qmrk: (Undef | 'a & ~undef) -> 'a -> 'a
//│          = [Function: qmrk_qmrk]

qmrk_qmrk example 0
//│ res: int
//│    = 0

qmrk_qmrk example "123"
//│ res: "123" | int
//│    = '123'

qmrk_qmrk undef "123"
//│ res: "123"
//│    = '123'

qmrk_qmrk 42 "123"
//│ res: "123" | 42
//│    = 42

