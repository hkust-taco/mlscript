//│     'd :> Var
//│        <: 'b & (Abs[?] & 'c | {name: string} & 'lhs & ~#Abs)
//│     'lhs :> (App['a] with {lhs: 'lhs, rhs: 'rhs}) | Var
//│          <: 'a & 'b
//│     'a :> (App['a] with {lhs: 'lhs, rhs: 'rhs}) | 'rhs
