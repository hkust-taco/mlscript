def foo b x = case b of {
  | Derived1 -> b.M2
  | Base1 -> b.M1 x
  }
//│ foo: (Base1['A] & ~derived1 | Derived1) -> 'A -> (Base1['A] | Derived1)
//│    = [Function: foo]

d1 = Derived1 { x = 1 }
