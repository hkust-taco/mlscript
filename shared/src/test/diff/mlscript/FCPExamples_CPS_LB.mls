// (* A function that receives a token *)
def f t = (s x x t x x e)
//│ f: 'b -> (forall 'b. ('c
//│   where
//│     'b <: () -> List["x"] -> (() -> (forall 'd. 'd -> (forall 'e. 'e -> (forall 'd, 'e, 'a, 'f. ('f
//│   where
//│     'd <: List['a]
//│     'e <: () -> List["x" | 'a] -> 'f))))) -> (() -> (forall 'd. 'd -> (forall 'e. 'e -> (forall 'd, 'e, 'a, 'f. ('f
//│   where
//│     'd <: List['a]
//│     'e <: () -> List["x" | 'a] -> 'f))))) -> (() -> List[string] -> () -> (forall 'g. 'g -> (forall 'h. 'h -> (forall 'g, 'h, 'a0, 'i. ('i
//│   where
//│     'g <: List['a0]
//│     'h <: () -> List['a0 | string] -> 'i))))) -> 'c))

// (* If the token is used twice, we must reveive two arguments *)
def g t1 t2 = (s x x t1 x (s x t2 x e) e)
//│ g: 'b -> (forall 'c. 'c -> (forall 'b, 'c. ('d
//│   where
//│     'b <: () -> List["x"] -> (() -> (forall 'e. 'e -> (forall 'f. 'f -> (forall 'e, 'f, 'a, 'g. ('g
//│   where
//│     'e <: List['a]
//│     'f <: () -> List["x" | 'a] -> 'g))))) -> 'h -> (() -> List[string] -> () -> (forall 'i. 'i -> (forall 'j. 'j -> (forall 'i, 'j, 'a0, 'k. ('k
//│   where
//│     'i <: List['a0]
//│     'j <: () -> List['a0 | string] -> 'k))))) -> 'd
//│     'c <: () -> List["x"] -> (() -> (forall 'e. 'e -> (forall 'f. 'f -> (forall 'e, 'f, 'a, 'g. ('g
//│   where
//│     'e <: List['a]
//│     'f <: () -> List["x" | 'a] -> 'g))))) -> (() -> List[string] -> () -> (forall 'i. 'i -> (forall 'j. 'j -> (forall 'i, 'j, 'a0, 'k. ('k
//│   where
//│     'i <: List['a0]
//│     'j <: () -> List['a0 | string] -> 'k))))) -> 'h)))

// (* This does not type. It requires first-class polymorphism. *)
def h t = g t t
//│ h: 'b -> (forall 'c, 'd. ('e
//│   where
//│     'c <: () -> List["x"] -> (() -> (forall 'f. 'f -> (forall 'g. 'g -> (forall 'f, 'g, 'a, 'h. ('h
//│   where
//│     'f <: List['a]
//│     'g <: () -> List["x" | 'a] -> 'h))))) -> 'i -> (() -> List[string] -> () -> (forall 'j. 'j -> (forall 'b. 'b -> (forall 'b, 'j, 'a0, 'k. ('k
//│   where
//│     'j <: List['a0]
//│     'b <: () -> List['a0 | string] -> 'k))))) -> 'e
//│     'd <: () -> List["x"] -> (() -> (forall 'f. 'f -> (forall 'g. 'g -> (forall 'f, 'g, 'a, 'h. ('h
//│   where
//│     'f <: List['a]
//│     'g <: () -> List["x" | 'a] -> 'h))))) -> (() -> List[string] -> () -> (forall 'j. 'j -> (forall 'b. 'b -> (forall 'b, 'j, 'a0, 'k. ('k
//│   where
//│     'j <: List['a0]
//│     'b <: () -> List['a0 | string] -> 'k))))) -> 'i))

