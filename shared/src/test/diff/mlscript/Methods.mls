//│ Declared True2.Test: True2['A, 'B] -> anything -> bool
//│ Defined True2.True: True2['A, 'B] -> bool
//│ Defined True2.Test: True2['A, 'B] -> anything -> true

p = Pair { x = 42; y = true }
fx = fun x -> mul x 2
fy = fun x -> not x
ft = fun x -> fun y -> if (y) then gt x 0 else y
