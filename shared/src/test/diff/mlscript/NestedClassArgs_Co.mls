class C2[A]: { a: A }
//│ Defined class C2[+A]


def mkC: 'a -> C2['a]
//│ mkC: 'a -> C2['a]

mkC' a = C2 { a }
//│ mkC': 'a -> C2['a]

mkC = mkC'
//│ 'a -> C2['a]
//│   <:  mkC:
//│ 'a -> C2['a]

rec def rc = mkC(rc)
