
// * Here the two occurrences of `'a` in different types are separate:
def foo(f: (~'a) -> 'a, a: 'a) =
  f a
//│ foo: (~'a -> 'a, ~'a,) -> 'a
//│    = [Function: foo]

def foo(fa: ((~'a) -> 'a, 'a)) =
  fa.0 fa.1
//│ foo: ((~'a -> 'a, 'a,),) -> 'a
//│    = [Function: foo1]

:ns
foo
//│ res: forall 'a '1 'b. ((~'a -> 'a, 'a,),) -> 'b
//│   where
//│     'a <: 'b & '1
//│     '1 <: ~'a
//│    = [Function: foo1]

