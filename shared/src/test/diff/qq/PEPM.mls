fun even(n) = n % 2 == 0
//│ fun even: Int -> Bool

// x^n
fun power(x, n) =
  if (n == 0) then 1
  else if (even(n)) then let t = power(x, Math.floor(n / 2)) in t * t
  else x * power(x, n - 1)
//│ fun power: (Int, Int) -> Int

power(3, 3)
power(2, 10)
//│ Int
//│ res
//│     = 27
//│ res
//│     = 1024


