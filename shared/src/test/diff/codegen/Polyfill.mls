:js
n + 1
//│ // Query 1
//│ res = n + 1;
//│ // End of generated code
//│ res: int
//│    = 124

// `withConstruct` should works on extended primitive values.
m = n with { y = 2 }
//│ m: 123 & {x: 1, y: 2}
//│  = [Number: 123] { x: 1, y: 2 }

// `m` should work well with following operations.
m.x
m.y
m + 1
//│ res: 1
//│    = 1
//│ res: 2
//│    = 2
//│ res: int
//│    = 124

p = m with { z = 9 }
p.x
p.y
p.z
p + 2
//│ p: 123 & {x: 1, y: 2, z: 9}
//│  = [Number: 123] { x: 1, y: 2, z: 9 }
//│ res: 1
//│    = 1
//│ res: 2
//│    = 2
//│ res: 9
//│    = 9
//│ res: int
//│    = 125

q = p with { x = 0; y = 0; z = 0 }
q.x
q.y
q.z
q + 3
//│ q: 123 & {x: 0, y: 0, z: 0}
//│  = [Number: 123] { x: 0, y: 0, z: 0 }
//│ res: 0
//│    = 0
//│ res: 0
//│    = 0
//│ res: 0
//│    = 0
//│ res: int
//│    = 126

