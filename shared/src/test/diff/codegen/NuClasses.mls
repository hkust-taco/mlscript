mixin M0(n: Int) {
  val m = n // this refers to specifically the `n` we had in parameter, not necessarily this.n
  fun bar = m
  fun foo = [n, m, bar] // should this be the same as `[this.n, this.m, this.bar]`?
}
//│ mixin M0(n: Int) {
//│   fun bar: Int
//│   fun foo: (Int, Int, Int,)
//│   let m: Int
//│ }

module M1 extends M0(123) {
  fun n = "n"
  fun m = "m"
  fun bar = "bar"
}
//│ module M1 {
//│   fun bar: "bar"
//│   fun foo: (Int, Int, Int,)
//│   fun m: "m"
//│   fun n: "n"
//│ }

[M1.n, M1.m, M1.bar]
//│ ("n", "m", "bar",)
//│ res
//│     = [ 'n', 'm', 'bar' ]

