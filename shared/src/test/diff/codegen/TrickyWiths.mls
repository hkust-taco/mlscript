//│ globalThis.n = withConstruct(42, { x: 1 });
//│ res = n;
//│ // Query 2
//│ res = n + n.x;
//│ // End of generated code
//│ n: 42 & {x: 1}
//│  = [Number: 42] { x: 1 }
//│ res: int
//│    = 43


