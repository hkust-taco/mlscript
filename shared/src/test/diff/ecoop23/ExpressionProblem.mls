//│ 'a -> int
//│   where
//│     'a <: Add['a] | Lit
//│ res
//│     = [Function: eval]

TestLang.eval(add11)
//│ int
//│ res
//│     = 3


mixin EvalNothing {
  fun eval(e) = e : nothing
}
mixin EvalAddLit {
  fun eval(e) =
    if e is
      Lit(n) then n: int
      Add(l, r) then this.eval(l) + this.eval(r)
    else super.eval(e)
}
module TestLang extends EvalNothing, EvalAddLit
