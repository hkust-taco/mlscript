:NoRecursiveTypes
:GeneralizeCurriedFunctions


def foo(f: forall 'a. 'a -> 'a) =
  (f 1, f true)
//│ foo: (forall 'a. 'a -> 'a) -> (1, true,)
//│    = [Function: foo]

fooid = foo id
//│ fooid: (1, true,)
//│      = [ 1, true ]

fooid._1
fooid._2
//│ res: 1
//│    = undefined
//│ res: true
//│    = undefined

def foo(f: (forall 'A. 'A -> 'A) -> (forall 'B. 'B -> 'B)) =
  id f id (f id)
//│ foo: ((forall 'A. 'A -> 'A) -> (forall 'B. 'B -> 'B)) -> (forall 'B. 'B -> 'B)
//│    = [Function: foo1]

foo id
//│ res: 'B -> 'B
//│    = [Function: id]

foo id id
//│ res: 'a -> 'a
//│    = [Function: id]



